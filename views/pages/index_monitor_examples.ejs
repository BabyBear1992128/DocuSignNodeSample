<%- include("../partials/genericHead") %>
<div class="jumbotron jumbotron-fluid">

<table>
    <tbody>
        <tr>
            <td>                
                <% if (locals.dsConfig.multiSourceChooser) { %>
                    <h1 class="display-4">Welcome</h1>
                    <p class="Xlead">Welcome to the code examples for DocuSign Monitor API with JWT Grant authentication</p>
                <% } else { %>
                    <h1 class="display-4">Node Launcher</h1>
                    <p class="Xlead">
                        <%- locals.manifest.SupportingTexts.HomePageText %>
                    </p>
                <% } %>

                </td>
                <td>
                    <img src="/images/banner-code.png" />
                </td>
            </tr>
        </tbody>
    </table>
</div>

<div class="container" style="margin-top: 40px" id="index-page">
    <% if (showDoc) { %>
        <p><a target='_blank' href='<%= documentation %>'>Documentation</a> on using OAuth Authorization Code Grant and JWT
            from
            a Node Express application.</p>
    <% } %>

    <%- include("index_monitor_examples_list") %>
</div>

<!-- anchor-js is only for the index page -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/anchor-js/4.1.1/anchor.min.js"></script>
<script>anchors.options.placement = 'left'; anchors.add('h4')</script>

<%- include("../partials/genericFoot") %>
